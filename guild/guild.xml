<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="GwGuildTabContainer" inherits="SecureHandlerBaseTemplate" protected="true" virtual="true">
        <Size x="853" y="627"></Size>
        <Anchors>
            <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0"></Anchor>
        </Anchors>
    </Frame>

    <Frame name="GwCommunitiesWindowTemplate" inherits="SecureHandlerStateTemplate,SecureHandlerAttributeTemplate,SecureHandlerShowHideTemplate" protected="true" virtual="true" enableMouse="true" hidden="true" frameStrata="HIGH" movable="true" resizable="true" toplevel="true">
        <HitRectInsets>
            <AbsInset left="-40" right="0" top="-40" bottom="-40"/>
        </HitRectInsets>
        <Size x="853" y="627"></Size>
        <Anchors>
            <Anchor point="TOP" relativeTo="GwSettingsMoverFrame" relativePoint="TOP" x="20" y="-40"></Anchor>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="background" file="Interface/AddOns/GW2_UI/textures/guild/guild-window-background">
                    <Size x="512" y="600"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0" />
                        <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="0" y="0" />
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bagheader">
                    <Size x="512" y="64"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="0" y="0" />
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bagheader-right">
                    <Size x="512" y="64"/>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" x="0" y="0" />
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bagfooter">
                    <Size x="512" y="64"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" x="0" y="2" />
                        <Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="2" />
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bagleftpanel">
                    <Size x="40" y="512"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativePoint="TOPLEFT" x="0" y="0" />
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer textureSubLevel="2">
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bottom-right">
                    <Size x="128" y="128"/>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer textureSubLevel="3">
                <Texture file="Interface/AddOns/GW2_UI/textures/guild/guild-shield-icon">
                    <Size x="84" y="84"></Size>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="TOPLEFT" x="18" y="-22" />
                    </Anchors>
                </Texture>
                <FontString parentKey="WindowHeader" text="GUILD_AND_COMMUNITIES" inherits="GameFontNormalSmall" justifyH="LEFT">
                    <Size x="512" y="40"/>
                    <Color r="1" b="0.73" g="0.93"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="30" y="40"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Button parentKey="secure" name="$parentClick" inherits="SecureHandlerClickTemplate" hidden="true">
            </Button>

            <Frame parentKey="mover" enableMouse="true" inherits="SecureHandlerMouseUpDownTemplate">
                <Size y="40" />
                <Anchors>
                    <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="-40" y="0"></Anchor>
                    <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" x="-30" y="0"></Anchor>
                </Anchors>
            </Frame>

            <Frame parentKey="sizer" enableMouse="true">
                <Size x="32" y="32"></Size>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"></Anchor>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY">
                        <Texture parentKey="texture" file="Interface/AddOns/GW2_UI/textures/uistuff/resize">
                            <Size x="32" y="32"></Size>
                            <Anchors>
                                <Anchor point="BOTTOMRIGHT" x="0" y="0"></Anchor>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
            
            <Button name="$parentClose" parentKey="close" inherits="SecureHandlerClickTemplate" text="" framelevel="10">
                <Size x="20" y="20" />
                <Anchors>
                    <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-10" y="30"/>
                </Anchors>
                <NormalTexture file="Interface/AddOns/GW2_UI/textures/uistuff/window-close-button-normal"></NormalTexture>
                <PushedTexture file="Interface/AddOns/GW2_UI/textures/uistuff/window-close-button-hover"></PushedTexture >
                <HighlightTexture file="Interface/AddOns/GW2_UI/textures/uistuff/window-close-button-hover"></HighlightTexture >

                <NormalFont style="ChatBubbleFont"></NormalFont>
                <HighlightFont style="ChatBubbleFont"></HighlightFont>

            </Button>
        </Frames>
    </Frame>
</Ui>